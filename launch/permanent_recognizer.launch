	<node pkg="voice_recognition" name="recognizer1" type="recognizer1.py" respawn="true" />
	<node pkg="voice_recognition" name="recognizer2" type="recognizer2.py" respawn="true" />
	<node pkg="voice_recognition" name="recognizer3" type="recognizer3.py" respawn="true" />
